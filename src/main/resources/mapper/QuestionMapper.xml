<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.QuestionMapper">

	<select id="getQuestionsBySurveyId" parameterType="int" resultType="com.example.demo.model.Question">
		SELECT * FROM question WHERE survey_id = #{surveyId} ORDER BY q_num ASC
	</select>

</mapper>