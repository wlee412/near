<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.SurveyMapper">


	<select id="getSurveyById" parameterType="int"
		resultType="com.example.demo.model.Survey">
		SELECT * FROM survey WHERE survey_id = #{surveyId}
	</select>


	<select id="getSurveysByIds" parameterType="list" resultType="com.example.demo.model.Survey">
		SELECT * FROM survey
		WHERE survey_id IN
		<foreach item="id" collection="list" open="(" separator=","
			close=")">
			#{id}
		</foreach>
	</select>
</mapper>